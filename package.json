{
  "name": "docs",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write **/.vitepress",
    "docs:dev": "vitepress dev ./docs --host",
    "docs:build": "vitepress build ./docs",
    "docs:preview": "vitepress preview ./docs"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "prettier": "^3.6.2",
    "vitepress": "^1.6.4"
  }
}
